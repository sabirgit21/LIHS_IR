<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="Inv_QC_Master_LOV"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  SelectList="TblQcM.ID, 
       TblQcM.DATED, 
       TblQcM.VNO, 
       TblQcM.PROJECT_ID"
  FromList="TBL_QC_M TblQcM"
  Where="tblqcm.id not in (select QC_M_ID From Tbl_GRN_M Where QC_M_ID is not null)  
and TblQcM.PROJECT_ID = :getProID">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.Inv_QC_Master_LOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="getProID"
    Kind="where"
    Type="java.math.BigDecimal">
    <TransientExpression
      Name="VariableScript"
      trustMode="trusted"
      CodeSourceName="Inv_QC_Master_LOV"/>
  </Variable>
  <EntityUsage
    Name="TblQcM"
    Entity="model.ERP.EO.TblQcM"/>
  <ViewAttribute
    Name="Id"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="TblQcM"
    AliasName="ID"/>
  <ViewAttribute
    Name="Dated"
    PrecisionRule="true"
    EntityAttrName="Dated"
    EntityUsage="TblQcM"
    AliasName="DATED">
    <Properties>
      <SchemaBasedProperties>
        <FMT_FORMAT
          ResId="model.LOV.Inv_QC_Master_LOV.Dated_FMT_FORMAT"/>
        <FMT_FORMATTER
          ResId="model.LOV.Inv_QC_Master_LOV.Dated_FMT_FORMATTER"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Vno"
    PrecisionRule="true"
    EntityAttrName="Vno"
    EntityUsage="TblQcM"
    AliasName="VNO"/>
  <ViewAttribute
    Name="ProjectId"
    PrecisionRule="true"
    EntityAttrName="ProjectId"
    EntityUsage="TblQcM"
    AliasName="PROJECT_ID"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
