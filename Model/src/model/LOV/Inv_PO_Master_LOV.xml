    Entity="model.ERP.EO.TblPoM"/>
  <EntityUsage
    Name="TblPblSupp"
    Entity="model.ERP.EO.TblPblSupp"
    ReadOnly="true"
    DeleteParticipant="false">
    <DesignTime>
      <Attr Name="_customjoin" Value="true"/>
    </DesignTime>
  </EntityUsage>
  <EntityUsage
    Name="TblProject"
    Entity="model.ERP.EO.TblProject"
    ReadOnly="true"
    DeleteParticipant="false">
    <DesignTime>
      <Attr Name="_customjoin" Value="true"/>
    </DesignTime>
  </EntityUsage>
  <ViewAttribute
    Name="Id"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="TblPoM"
    AliasName="ID"/>
  <ViewAttribute
    Name="Dated"
    PrecisionRule="true"
    EntityAttrName="Dated"
    EntityUsage="TblPoM"
    AliasName="DATED">
    <Properties>
      <SchemaBasedProperties>
        <FMT_FORMAT
          ResId="model.LOV.Inv_PO_Master_LOV.Dated_FMT_FORMAT"/>
        <FMT_FORMATTER
          ResId="model.LOV.Inv_PO_Master_LOV.Dated_FMT_FORMATTER"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Vno"
    PrecisionRule="true"
    EntityAttrName="Vno"
    EntityUsage="TblPoM"
    AliasName="VNO"/>
  <ViewAttribute
    Name="PblSuppId"
    PrecisionRule="true"
    EntityAttrName="PblSuppId"
    EntityUsage="TblPoM"
    AliasName="PBL_SUPP_ID"/>
  <ViewAttribute
    Name="Name"
    PrecisionRule="true"
    EntityAttrName="Name"
    EntityUsage="TblPblSupp"
    AliasName="NAME"/>
  <ViewAttribute
    Name="Id1"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="TblPblSupp"
    AliasName="ID1"/>
  <ViewAttribute
    Name="ProjectId"
    PrecisionRule="true"
    EntityAttrName="ProjectId"
    EntityUsage="TblPoM"
    AliasName="PROJECT_ID"/>
  <ViewAttribute
    Name="Name1"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Name"
    EntityUsage="TblProject"
    AliasName="NAME1"/>
  <ViewAttribute
    Name="Id2"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="TblProject"
    AliasName="ID2"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
