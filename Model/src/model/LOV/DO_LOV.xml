<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="DO_LOV"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  SelectList="TblDoM.DATED, 
       TblDoM.ID, 
       TblDoM.PROJECT_ID"
  FromList="TBL_DO_M TblDoM"
  Where="TblDoM.PROJECT_ID = :getProID">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.DO_LOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="getProID"
    Kind="where"
    Type="java.math.BigDecimal">
    <TransientExpression
      Name="VariableScript"
      trustMode="trusted"
      CodeSourceName="DO_LOV"/>
  </Variable>
  <EntityUsage
    Name="TblDoM"
    Entity="model.ERP.EO.TblDoM"/>
  <ViewAttribute
    Name="Dated"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Dated"
    EntityUsage="TblDoM"
    AliasName="DATED"/>
  <ViewAttribute
    Name="Id"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="TblDoM"
    AliasName="ID"/>
  <ViewAttribute
    Name="ProjectId"
    PrecisionRule="true"
    EntityAttrName="ProjectId"
    EntityUsage="TblDoM"
    AliasName="PROJECT_ID"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
