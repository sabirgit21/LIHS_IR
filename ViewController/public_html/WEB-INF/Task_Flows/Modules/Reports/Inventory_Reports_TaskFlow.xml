<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="Inventory_Reports_TaskFlow">
    <default-activity>Inventory_Reports</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>InventoryReportsBackBean</managed-bean-name>
      <managed-bean-class>view.Report.InventoryReports</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <view id="Inventory_Reports">
      <page>/WEB-INF/Fragments/Modules/Reports/Inventory_Reports.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
