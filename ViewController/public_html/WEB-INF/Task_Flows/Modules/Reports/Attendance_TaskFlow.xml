<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="Attendance_TaskFlow">
    <default-activity>Attendance</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>AttendanceReportBackBean</managed-bean-name>
      <managed-bean-class>view.Report.AttendanceReport</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <view id="Attendance">
      <page>/WEB-INF/Fragments/Modules/Reports/Attendance.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
